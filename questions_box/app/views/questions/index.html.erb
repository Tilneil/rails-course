<div>
    <% @questions.each do |question| %>
        <div>
            <h3>Question: <%= question.q_title %></h3>
        </div>
        <div>
            <h4>Answer</h4>
            <p><%= question.answer %></p>
        </div>
   
        <div>
             <h5>For more information visit:</h5><%= question.resource_name %></h5>       
        </div>
        <div>
        
        <div>
            <%= question.web_link %>
        </div>
        <div>
            <%= link_to 'Edit', edit_question_path(question) %>
        </div>
        <div>
           <%= link_to "Delete", question_path(question), method: :delete %>
        </div>
    <% end %>
</div>
